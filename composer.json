{
    "name": "NijmegenSync/DS-Geoserver",
    "description": "Contains the functionality to harvest the Nijmegen geoserver.",
    "keywords": [],
    "homepage": "https://github.com/NijmegenSync/DataSource-Geoserver",
    "readme": "README.md",
    "abandoned": false,
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Willem ter Berg",
            "email": "wrpterberg@gmail.com",
            "homepage": "https://github.com/WterBerg",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "github",
            "url": "https://github.com/NijmegenSync/Contracts",
            "no-api": true
        },
        {
            "type": "github",
            "url": "https://github.com/NijmegenSync/DataSource",
            "no-api": true
        }
    ],
    "autoload": {
        "psr-4": {
            "GeoserverDataSource\\": "src/"
        }
    },
    "config": {
        "platform": {
            "php": "7.2.7"
        },
        "discard-changes": true,
        "preferred-install": "dist"
    },
    "scripts": {
        "test": "phpunit --bootstrap vendor/autoload.php --testdox test/"
    },
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "doctrine/orm": "2.4.*",
        "guzzlehttp/guzzle": "^6.2",
        "symfony/filesystem": "^4",
        "NijmegenSync/Contracts": "dev-master",
        "NijmegenSync/DataSource": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^7"
    }
}
